      <!--main content start-->
      <form id="form1" name="form1">
          <input type="hidden" id="artist_sn" name="artist_sn" value="<%=artist_sn %>">
      </form>
      <section id="main-content">
          <section class="wrapper">
              <!--main-content-title start-->
            <div class="main-content-title">
                  <h1 class="eng"><strong>Artist history</strong></h1>
                  <h5 class="sub-title korean">[작가 약력 수정]</h5>
              </div>
        <!--/.main-content-title end-->
              <!--history_edit start-->
              <div class="history_edit korean col-lg-12">
                  <form ng-submit="addTodo()">
                      <div class="col-lg-2">
                          <input type="text"  class="form-control input-lg" ng-model="todoYear" size="20" placeholder="년">
                      </div>
                      <div class="col-lg-1">
                          <input type="text"  class="form-control input-lg" ng-model="todoMonth" size="20" placeholder="월">
                      </div>
                      <div class="col-lg-7">
                          <input type="text"  class="form-control input-lg" ng-model="todoContent" size="30" placeholder="내용을 입력해주세요">
                      </div>
                      <div class="history_add_btn col-lg-2">
                          <button class="btn btn-theme06 korean" type="submit">약력추가하기</button>
                      </div>
                  </form>
                  <!--add_history start-->
                  <div class="add_history korean col-lg-12">
                      <ul class="unstyled col-lg-12">
                          <li ng-repeat="todo in todos" class="add_history_list col-lg-12">
                              <div class="col-lg-2" >
                                  <strong>[<span class="done-{{todo.done}}">{{todo.year}}.{{todo.month}}</span>]</strong>
                              </div>
                              <div class="col-lg-8">
                                  <span class="done-{{todo.done}}">{{todo.content}}</span>
                              </div>
                              <div class="col-lg-2">
                                  <input type="checkbox"  class="" ng-model="todo.done">
                              </div>
                          </li>
                      </ul>
                  </div>
                  <!--/.add_history end-->
                  <div class="korean del_history">
                      <a href="" ng-click="remove()" class="korean btn btn-theme04">체크된 약력 삭제하기</a>
                  </div>
              </div>
              <!--/.history_edit end-->
          </section>
          <div class="edit_complete">
              <form>
                  <a data-toggle="modal" href="#myModal"><button type="button" class="edit_complete_btn korean">수정완료</button></a>
              </form>
          </div>
      </section>
    <!--main content end-->
      <!-- Modal -->
    <div aria-hidden="true" aria-labelledby="myModalLabel" role="dialog" tabindex="-1" id="myModal" class="modal fade">
