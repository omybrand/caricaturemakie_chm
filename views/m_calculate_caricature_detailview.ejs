      <section id="main-content">
          <section class="wrapper">
	  	      <div class="main-content-title">
              	  <h1 class="eng"><strong>Calculate caricature detailview</strong></h1>
                  <h5 class="sub-title korean">[캐리커쳐 정산 상세정보]</h5>
              </div>
              <!--calculate_info start-->
              <div class="calculate_info">
                <form>
                  <div class="form-group col-lg-12">
                  	  <div class="col-lg-4 discount_price">
                      	  <select class="korean form-control input-lg" ng-change="findTodo()" ng-model="formData.yy">
                               <% for (var i = 2014; i <= yy; i++) { %>
                                  <% if (i == yy) { %>
                                    <option value=<%=i %> selected><%=i %>년</option>
                                  <% } else { %>
                                    <option value=<%=i %>><%=i %>년</option>
                                  <% } %>
                              <% } %>
                          </select>
                      </div>
                      <div class="col-lg-4 discount_price">
                          <select class="korean form-control input-lg" ng-change="findTodo()" ng-model="formData.mm">
                              <% for (var i = 1; i <= 12; i++) { %>
                                  <% if (i == mm) { %>
                                      <option value=<%=i %> selected><%=i %>월</option>
                                  <% } else { %>
                                      <option value=<%=i %>><%= i%>월</option>
                                  <% } %>
                              <% } %>
                          </select>
                      </div>
                      <div class="col-lg-4 discount_price">
                          <select class="korean form-control input-lg" ng-change="findTodo()" ng-model="formData.artist_sn">
                              <% for (var i in datas) { %>
                                <% if (i == 0) { %>
                                  <option value=<%=datas[i].artist_sn %> selected><%=datas[i].artist_name %></option>
                                <% } else {  %>
                                  <option value=<%=datas[i].artist_sn %>><%=datas[i].artist_name %></option>
                                <% } %>
                              <% } %>
                          </select>
                          <input type="hidden" id="year" value="<%=yy %>">
                          <input type="hidden" id="month" value="<%=mm %>">
                          <input type="hidden" id="artist_sn" value="<%=datas[0].artist_sn %>">
                      </div>
                  </div>
                  </form>
                  <!--calculate app start(개인판매정보)-->
                  <div class="calculate app col-lg-12">
                  	  <h4 class="korean">[App,오프라인판매]</h4>
                      <!--panel (판매된 캐리커쳐 정보) start-->
                      <div class="panel panel-default col-lg-12" ng-repeat="todo in todos">
                          <!--panel-heading start-->
                          <div class="panel-heading_w col-lg-12">
                              <!--panel-title start-->
                              <h4 class="panel-title korean">
                                 <div class="col-lg-2">
                                    <p>{{todo.order_gettime}}</p>
                                 </div>
                                 <div class="col-lg-2">
                                    <p>{{todo.style_name}}</p>
                                 </div>
                                 <div class="col-lg-2">
                                    <p>{{todo.mode}}</p>
                                 </div>
                                 <div class="col-lg-2">
                                    <p>{{todo.order_pcount}}인</p>
                                 </div>
                                 <div class="col-lg-2">
                                    <p>{{todo.o_size}}</p>
                                 </div>
                                 <div class="col-lg-2">
                                    <p>{{todo.price}}원</p>
                                 </div>
                              </h4>
                              <!--/.panel-title end-->
                          </div>
                          <!--/.panel-heading end-->
                      </div>
                      <!--/.panel (판매된 캐리커쳐 정보) end-->
                  </div>
                  <!--/.calculate app end(개인판매정보)-->
                  <!--calculate open_market start(오픈마켓 판매정보)-->
