        function submit() {
          if (document.form1.style_name.value == '') {
            alert("스타일명을 입력하지 않았습니다.");
            return;
          }
          if (document.form1.style_adjective.value == '') {
            alert("수식어구를 입력하지 않았습니다.");
            return;
          }
          if (document.form1.style_description.value == '') {
            alert("설명을 입력하지 않았습니다.");
            return;
          }
          if (document.form1.style_onesketch.value == '' || document.form1.style_onesketch.value < 0) {
            alert("1인 상반신 스케치 가격이 올바르지 않습니다.");
            return;
          }
          if (document.form1.style_onepointcolor.value == '' || document.form1.style_onepointcolor.value < 0) {
            alert("1인 상반신 포인트컬러 가격이 올바르지 않습니다.");
            return;
          }
          if (document.form1.style_onecolor.value == '' || document.form1.style_onecolor.value < 0) {
            alert("1인 상반신 풀컬러 가격이 올바르지 않습니다.");
            return;
          }
          if (document.form1.style_onefullsketch.value == '' || document.form1.style_onefullsketch.value < 0) {
            alert("1인 전신 스케치 가격이 올바르지 않습니다.");
            return;
          }
          if (document.form1.style_onefullpointcolor.value == '' || document.form1.style_onefullpointcolor.value < 0) {
            alert("1인 전신 포인트컬러 가격이 올바르지 않습니다.");
            return;
          }
          if (document.form1.style_onefullcolor.value == '' || document.form1.style_onefullcolor.value < 0) {
            alert("1인 전신 풀컬러 가격이 올바르지 않습니다.");
            return;
          }
          if (document.form1.style_add1p.value == '') {
            alert("2인 할인율을 입력하지 않았습니다.");
            return;
          }
          if (document.form1.style_add1p.value < 0 || document.form1.style_add1p.value > 100) {
            alert("2인 할인율이 올바르지 않습니다.");
            return;
          }
          if (document.form1.style_add2p.value == '') {
            alert("3인 할인율을 입력하지 않았습니다.");
            return;
          }
          if (document.form1.style_add2p.value < 0 || document.form1.style_add2p.value > 100) {
            alert("3인 할인율이 올바르지 않습니다.");
            return;
          }
          if (document.form1.style_add3p.value == '') {
            alert("4인 할인율을 입력하지 않았습니다.");
            return;
          }
          if (document.form1.style_add3p.value < 0 || document.form1.style_add3p.value > 100) {
            alert("4인 할인율이 올바르지 않습니다.");
            return;
