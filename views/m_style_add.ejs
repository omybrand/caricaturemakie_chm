            return;
          }
          document.form1.submit();
        }

        function calcprice() {
          if (document.form1.style_onesketch.value == '' || document.form1.style_onesketch.value < 0) {
            alert("1인 상반신 스케치 가격이 올바르지 않습니다.");
            return;
          }
          if (document.form1.style_onepointcolor.value == '' || document.form1.style_onepointcolor.value < 0) {
            alert("1인 상반신 포인트컬러 가격이 올바르지 않습니다.");
            return;
          }
          if (document.form1.style_onecolor.value == '' || document.form1.style_onecolor.value < 0) {
            alert("1인 상반신 풀컬러 가격이 올바르지 않습니다.");
            return;
          }
          if (document.form1.style_onefullsketch.value == '' || document.form1.style_onefullsketch.value < 0) {
            alert("1인 전신 스케치 가격이 올바르지 않습니다.");
            return;
          }
          if (document.form1.style_onefullpointcolor.value == '' || document.form1.style_onefullpointcolor.value < 0) {
            alert("1인 전신 포인트컬러 가격이 올바르지 않습니다.");
            return;
          }
          if (document.form1.style_onefullcolor.value == '' || document.form1.style_onefullcolor.value <  0) {
            alert("1인 전신 풀컬러 가격이 올바르지 않습니다.");
            return;
          }
          if (document.form1.style_add1p.value == '') {
            alert("2인 할인율을 입력하지 않았습니다.");
            return;
          }
          if (document.form1.style_add1p.value < 0 || document.form1.style_add1p.value > 100) {
            alert("2인 할인율이 올바르지 않습니다.");
            return;
          }
          if (document.form1.style_add2p.value == '') {
            alert("3인 할인율을 입력하지 않았습니다.");
            return;
          }
          if (document.form1.style_add2p.value < 0 || document.form1.style_add2p.value > 100) {
            alert("3인 할인율이 올바르지 않습니다.");
            return;
          }
          if (document.form1.style_add3p.value == '') {
            alert("4인 할인율을 입력하지 않았습니다.");
            return;
          }
          if (document.form1.style_add3p.value < 0 || document.form1.style_add3p.value > 100) {
            alert("4인 할인율이 올바르지 않습니다.");
            return;
          }
            var prices = [];
            var price1 = Number($('#style_onesketch').val());
            var price2 = Number($('#style_onepointcolor').val());
            var price3 = Number($('#style_onecolor').val());
            var price4 = Number($('#style_onefullsketch').val());
            var price5 = Number($('#style_onefullpointcolor').val());
            var price6 = Number($('#style_onefullcolor').val());
            var add1p = Number($('#style_add1p').val());
            var add2p = Number($('#style_add2p').val());
            var add3p = Number($('#style_add3p').val());
            prices.push(price1 + price1 * add1p / 100);
            prices.push(price1 + price1 * add1p / 100 + price1 * add2p / 100);
            prices.push(price1 + price1 * add1p / 100 + price1 * add2p / 100 + price1 * add3p / 100);

            prices.push(price2 + price2 * add1p / 100);
            prices.push(price2 + price2 * add1p / 100 + price2 * add2p / 100);
            prices.push(price2 + price2 * add1p / 100 + price2 * add2p / 100 + price2 * add3p / 100);

            prices.push(price3 + price3 * add1p / 100);
            prices.push(price3 + price3 * add1p / 100 + price3 * add2p / 100);
            prices.push(price3 + price3 * add1p / 100 + price3 * add2p / 100 + price3 * add3p / 100);

            prices.push(price4 + price4 * add1p / 100);
            prices.push(price4 + price4 * add1p / 100 + price4 * add2p / 100);
            prices.push(price4 + price4 * add1p / 100 + price4 * add2p / 100 + price4 * add3p / 100);

            prices.push(price5 + price5 * add1p / 100);
            prices.push(price5 + price5 * add1p / 100 + price5 * add2p / 100);
            prices.push(price5 + price5 * add1p / 100 + price5 * add2p / 100 + price5 * add3p / 100);

            prices.push(price6 + price6 * add1p / 100);
            prices.push(price6 + price6 * add1p / 100 + price6 * add2p / 100);
            prices.push(price6 + price6 * add1p / 100 + price6 * add2p / 100 + price6 * add3p / 100);

            for (var i in prices) {
              console.log(prices[i]);
              if (prices[i] > 0) {
                  $('#calresult' + i).html('');
                  $('#calresult' + i).html('<p><span class="hidden-lg">[2인]</span>' + String(prices[i]).comma() + '원</p>');
              } else {
                  $('#calresult' + i).html('');
                  $('#calresult' + i).html('<p><span class="hidden-lg">[2인]</span>-</p>');
              }
            }
        }

        String.prototype.comma = Number.prototype.comma = function() {
            var n = String(this).replace(/\,/g,'');
            return n.match(RegExp('^[0-9]{'+(n.length%3||3)+'}|[0-9]{3}','g')).join();
        };
    </script>

