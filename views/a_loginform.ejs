<!DOCTYPE html>
<html lang="ko">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="omybrand_artist_admin">
    <meta name="author" content="omybrand">

	<title>Caricature Ma-kie Artist</title>

    <!-- Custom styles for this template -->
    <link href="/chm/assets_artist/css/style.css" rel="stylesheet">
    <link href="/chm/assets_artist/css/style-responsive.css" rel="stylesheet">

    <!-- Bootstrap core CSS -->
    <link href="/chm/assets_artist/css/bootstrap.css" rel="stylesheet">
    <!--external css-->
    <link href='http://fonts.googleapis.com/css?family=Comfortaa:400,300,700' rel='stylesheet' type='text/css'>
    <link href="/chm/assets_artist/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="/chm/assets_artist/css/zabuto_calendar.css">
    <link rel="stylesheet" type="text/css" href="/chm/assets_artist/js/gritter/css/jquery.gritter.css" />
    <link rel="stylesheet" type="text/css" href="/chm/assets_artist/lineicons/style.css">

    <script>
        function login() {
            if (document.getElementById('artist_id').value == '') {
                alert('아이디를 입력해 주세요!');
                document.getElementById('artist_id').focus();
                return;
            }
            if (document.getElementById('artist_pwd').value == '') {
                alert('비밀번호를 입력해 주세요!');
                document.getElementById('artist_pwd').focus();
                return;
            }
            document.loginform.submit();
        }
    </script>

    <script src="/chm/assets_artist/js/chart-master/Chart.js"></script>

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>
<body>
<!-- Page Content -->
    <div class="container" >
    	 <!-- Content Row -->
        <div class="row" id="login">
        	<!--logo start-->
            <div class="login_logo"><img src="/chm/assets_artist/img/login_logo_2.png"></div>
            <!--logo end-->
            <div class="col-lg-12">
                <form role="form" id="loginform" name="loginform" action="/chm/artist/login" method="post">
                    <div class="form-group">
                        <label for="artist_id"></label>
                        <input type="email" class="form-control text-center eng input-lg" id="artist_id" name="artist_id" placeholder="ID">
                    </div>
                    <div class="form-group">
                        <label for="artist_pwd"></label>
                        <input type="password" class="form-control text-center eng input-lg" id="artist_pwd" name="artist_pwd" placeholder="Password">
                    </div>
                    <input type="button" class="btn login_btn korean" value="로그인" onclick="login()"/>
                </form>
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
    </div>

    <!-- jQuery Version 1.11.0 -->
    <script src="js/jquery-1.11.0.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="js/bootstrap.min.js"></script>

</body>

</html>
