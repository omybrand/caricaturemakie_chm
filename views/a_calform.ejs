      <!-- **********************************************************************************************************************************************************
      MAIN CONTENT
      *********************************************************************************************************************************************************** -->
      <!--main content start-->
      <section id="main-content">
          <section class="wrapper">
	  	      <!--calculate_info start-->
              <div class="calculate_info">
                  <div class="form-group col-lg-12" id="calculate_result">
                    <form>
                  	  <div class="col-lg-6 discount_price">
                      	<select class="korean form-control input-lg" ng-change="findTodo()" ng-model="formData.yy">
                          	   <% for (var i = 2014; i <= yy; i++) { %>
                                  <% if (i == yy) { %>
                                    <option value=<%=i %> selected><%=i %>년</option>
                                  <% } else { %>
                                    <option value=<%=i %>><%=i %>년</option>
                                  <% } %>
                              <% } %>
                          </select>
                      </div>
                      <div class="col-lg-6 discount_price">
                          <select class="korean form-control input-lg" ng-change="findTodo()" ng-model="formData.mm">
                          	  <% for (var i = 1; i <= 12; i++) { %>
                                  <% if (i == mm) { %>
                                      <option value=<%=i %> selected><%=i %>월</option>
                                  <% } else { %>
                                      <option value=<%=i %>><%= i%>월</option>
                                  <% } %>
                              <% } %>
                          </select>
                      </div>
                      <input type="hidden" id="year" value=<%=yy %>/>
                      <input type="hidden" id="month" value=<%=mm %>/>
                  </div>
                </form>
                  <!--panel (판매된 캐리커쳐 정보) start-->
                  <div ng-repeat="todo in todos">
                    <div class="panel panel-default col-lg-12">
                        <!--panel-heading start-->
                        <div class="panel-heading_w col-lg-12">
                            <!--panel-title start-->
                            <h4 class="panel-title korean">
                               <div class="col-lg-2">
                               	<p>{{todo.order_gettime}}</p>
                               </div>
                               <div class="col-lg-2">
                                  <p>{{todo.style_name}}</p>
                               </div>
                               <div class="col-lg-2">
                                  <p>{{todo.mode}}</p>
                               </div>
                               <div class="col-lg-2">
                                  <p>{{todo.order_pcount}}인</p>
                               </div>
                               <div class="col-lg-2">
                                  <p>{{todo.o_size}}</p>
                               </div>
                               <div class="col-lg-2">
                                  <p>{{todo.price}}원</p>
                               </div>
                            </h4>
                            <!--/.panel-title end-->
                        </div>
                        <!--/.panel-heading end-->
                    </div>
                    <!--/.panel (판매된 캐리커쳐 정보) end-->
                  </div>
              </div>
              <!--/.calculate_info end-->
              <!--calculate_result start-->
              <div class="calculate_result korean col-lg-12" id="calculate_result">
              	  <div class="calculate_result_caption_pink">
                  	  <ul>
                      	  <li><h5 class="calculate_result_title korean">합계</h5></li>
                      </ul>
                  </div>
                  <div class="calculate_result_body col-lg-12">
                  	  <div class="col-lg-12 calculate_result_info">
                       	  <p><span>총 판매개수</span> : <span>{{cnt}}개</span></p>
                      </div>
                      <div class="col-lg-12 calculate_result_info">
                       	  <p><span>총 판매금액</span> : <span>{{sum}}원</span></p>
                      </div>
                      <!--
                      <div class="col-lg-12 calculate_result_info">
                       	  <p><span>판매수수료+세금+PG사 수수료</span> : <span>12,000원</span></p>
                      </div>
                      <hr class="col-lg-12" id="hr_margin_row">
                      <div class="col-lg-12 calculate_result_info">
                       	  <p class="proceeds"><span>총 수익금</span> : <span>18,000원</span></p>
                      </div>
                    -->
                  </div>
              </div>
              <!--/.calculate_result end-->
          </section>
      </section>
	  <!--main content end-->
  </section>

