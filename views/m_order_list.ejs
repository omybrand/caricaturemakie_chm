      <section id="main-content">
          <section class="wrapper">
	  	      <div class="main-content-title">
              	  <h1 class="eng"><strong>Order list</strong></h1>
                  <h5 class="sub-title korean">[주문내역 관리]</h5>
              </div>
              <form>
                  <!--user_edit start-->
                  <div class="user_edit">
                      <div class="order_select col-lg-4">
                      	<select class="form-control input-lg korean" ng-change="findTodo()" ng-model="formData.mode">
                        	<option value="0">전체 주문보기</option>
                            <option value="1">결제된 주문보기</option>
                            <option value="2">완료된 주문보기</option>
                        </select>
                      </div>
                      <div class="order_select col-lg-4">
                      	<select class="form-control input-lg korean" ng-change="findTodo()" ng-model="formData.yy">
                          <% for (var i = 2014; i <= yy; i++) { %>
                            <% if (i == yy) { %>
                              <option value=<%=i %> selected><%=i %>년</option>
                            <% } else { %>
                              <option value=<%=i %>><%=i %>년</option>
                            <% } %>
                          <% } %>
                        </select>
                      </div>
                      <div class="order_select col-lg-4">
                          <select class="form-control input-lg korean" ng-change="findTodo()" ng-model="formData.mm">
                            <% for (var i = 1; i <= 12; i++) { %>
                              <% if (i == mm) { %>
                                <option value=<%=i %> selected><%=i %>월</option>
                                <% } else { %>
                                  <option value=<%=i %>><%= i%>월</option>
                                <% } %>
                                  <% } %>
                              </select>
                      </div>
                      <input type="hidden" id="mode" value="0"/>
                      <input type="hidden" id="year" value=<%=yy %>/>
                      <input type="hidden" id="month" value=<%=mm %>/>
                  </form>
                  <!--panel-->
                  <div class="panel panel-default col-lg-12" ng-repeat="todo in todos">
                      <!--panel-heading start-->
                      <div class="panel-heading_w col-lg-12">
                          <!--panel-title start-->
                          <h4 class="panel-title korean">
                             <div class="col-lg-3">
                             	<p class="black_id">{{todo.gettime}}</p>
                             </div>
                             <div class="col-lg-3">
                                <p>{{todo.user_id}}</p>
                             </div>
                             <div class="col-lg-3">
                                <p>{{todo.style_name}}</p>
                             </div>
                             <div class="col-lg-3 notice_btn_group">
                             	<a ng-href="/chm/manage/orderlist/info?sn={{todo.order_sn}}"><button type="button" class="notice_edit_btn btn-theme03 korean">상세보기</button></a>
                             </div>
                          </h4>
                          <!--/.panel-title end-->
                      </div>
                      <!--/.panel-heading end-->
                  </div>
                  <!--/.panel end-->
              </div>
              <!--/.user_edit end-->
          </section>
