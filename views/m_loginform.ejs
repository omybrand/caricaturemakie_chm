<!DOCTYPE html>
<html lang="ko">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>Omav_Chm_Admin </title>

    <!-- Custom styles for this template -->
    <link href="/chm/assets/css/style.css" rel="stylesheet">
    <link href="/chm/assets/css/style-responsive.css" rel="stylesheet">

    <!-- Bootstrap core CSS -->
    <link href="/chm/assets/css/bootstrap.css" rel="stylesheet">
    <!--external css-->
    <link href='http://fonts.googleapis.com/css?family=Comfortaa:400,300,700' rel='stylesheet' type='text/css'>
    <link href="/chm/assets/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="/chm/assets/css/zabuto_calendar.css">
    <link rel="stylesheet" type="text/css" href="/chm/assets/js/gritter/css/jquery.gritter.css" />
    <link rel="stylesheet" type="text/css" href="/chm/assets/lineicons/style.css">

    <script src="/chm/assets/js/chart-master/Chart.js"></script>

    <script>
        function login() {
            if (document.getElementById('admin_id').value == '') {
                alert('아이디를 입력해 주세요!');
                document.getElementById('admin_id').focus();
                return;
            }
            if (document.getElementById('admin_pwd').value == '') {
                alert('비밀번호를 입력해 주세요!');
                document.getElementById('admin_pwd').focus();
                return;
            }
            document.loginform.submit();
        }
    </script>

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>
<body>
<!-- Page Content -->
    <div class="container" >
         <!-- Content Row -->
        <div class="row" id="login">
            <!--logo start-->
            <div class="login_logo"><b>OMV<br>admin</b></div>
            <!--logo end-->
            <div class="col-lg-12">
                <form role="form" id="loginform" name="loginform" action="/chm/manage/login" method="post">
                  <div class="form-group">
                    <label for="AdminID"></label>
                    <input type="text" class="form-control text-center eng input-lg" id="admin_id" name="admin_id" placeholder="ID">
                  </div>
                  <div class="form-group">
                    <label for="AdminPassword"></label>
                    <input type="password" class="form-control text-center eng input-lg" id="admin_pwd" name="admin_pwd" placeholder="PW">
                  </div>
                  <button type="button" class="btn login_btn korean" onclick="login()">로그인</button>
                </form>
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
    </div>



    <!-- jQuery Version 1.11.0 -->
    <script src="/chm/assets/js/jquery-1.11.0.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="/chm/assets/js/bootstrap.min.js"></script>

</body>

</html>
